      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint"
    ]
  },
  "prettier": {
    "printWidth": 100
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
