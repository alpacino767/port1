    "pug": "3.0.2",
    "pug-loader": "2.4.0",
    "raw-loader": "4.0.2",
    "rimraf": "2.6.3",
    "semistandard": "^13.0.1",
    "standard-version": "^9.3.0",
    "style-loader": "2.0.0",
    "typescript": "4.1.3",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0",
    "webpack-recompilation-simulator": "3.2.0"
  },
  "dependencies": {
    "@types/html-minifier-terser": "^6.0.0",
    "html-minifier-terser": "^6.0.2",
    "lodash": "^4.17.21",
