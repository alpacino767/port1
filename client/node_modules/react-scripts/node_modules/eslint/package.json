    "@nodelib/fs.walk": "^1.2.8",
    "ajv": "^6.10.0",
    "chalk": "^4.0.0",
    "cross-spawn": "^7.0.2",
    "debug": "^4.3.2",
    "doctrine": "^3.0.0",
    "escape-string-regexp": "^4.0.0",
