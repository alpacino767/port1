    "is-glob": "^4.0.0",
    "is-path-inside": "^3.0.3",
    "js-sdsl": "^4.1.4",
    "js-yaml": "^4.1.0",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "levn": "^0.4.1",
    "lodash.merge": "^4.6.2",
    "minimatch": "^3.1.2",
    "natural-compare": "^1.4.0",
    "optionator": "^0.9.1",
    "strip-ansi": "^6.0.1",
    "strip-json-comments": "^3.1.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "c8": "^7.12.0",
    "chai": "^4.0.1",
    "cheerio": "^0.22.0",
