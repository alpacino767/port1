    "@types/fs-extra": "^9.0.13",
    "@types/micromatch": "^4.0.2",
    "@types/normalize-path": "^3.0.0",
    "@types/webpack": "^5.28.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.0.3",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
