{
  "name": "terser-webpack-plugin",
  "version": "5.3.7",
  "description": "Terser plugin for webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/terser-webpack-plugin.git"
  },
  "author": {
    "name": "webpack Contrib Team"
  },
