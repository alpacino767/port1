    "build": "pnpm clean && cross-env NODE_ENV=production pnpm webpack",
    "clean": "rm -rf dist",
    "dev": "pnpm clean && pnpm webpack --watch",
    "jiti": "cross-env JITI_DEBUG=1 JITI_CACHE=false JITI_REQUIRE_CACHE=false ./bin/jiti.js",
