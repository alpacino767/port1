    "react": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "sourcemap-validator": "^2.1.0",
    "swc-plugin-coverage-instrument": "^0.0.12",
    "terser": "^5.7.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@swc/helpers": "^0.5.0"
  },
