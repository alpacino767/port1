{
  "name": "schema-utils",
  "version": "2.7.0",
  "description": "webpack Validation Utils",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack/schema-utils.git"
  },
  "author": {
    "name": "webpack Contrib",
