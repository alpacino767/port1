    "babel-jest": "^25.5.1",
    "cross-env": "^6.0.3",
    "del": "^5.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.3"
  },
